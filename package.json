{
  "name": "youtubeapp",
  "version": "1.0.0",
  "description": "App for YouTube Stats",
  "main": "app.jsx",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "webpack --color --config webpack.production.config.js",
    "flow": "flow",
    "test-debug": "./node_modules/.bin/node-debug ./node_modules/.bin/jest -i --watch"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 57,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Kalada Opuiyo",
  "license": "ISC",
  "homepage": "/",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.16.1",
    "eslint-config-uber-es2015": "^3.1.1",
    "eslint-config-uber-jsx": "^3.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "exports-loader": "^0.6.4",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.4",
    "postcss-import": "9.1.0",
    "postcss-loader": "^1.3.3",
    "precss": "1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.5",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.21.0",
    "axios": "^0.17.0",
    "babel-register": "6.24.1",
    "body-parser": "^1.17.2",
    "es6-promise": "^4.1.0",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.1.1",
    "react-collapsible": "^1.5.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^15.6.2",
    "react-infinite": "^0.11.1",
    "react-router": "^3.0.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-spinkit": "^3.0.0"
  }
}
